

<% unless @advertiser.checklist_complete? %>
  <%= render "layouts/dashboard/checklist", advertiser: @advertiser %>
<% end %>

<% if @advertiser.subscription.nil? %>
  <%= render "layouts/dashboard/subscription", subscription: @advertiser.subscription %>
<% end %>


<% @advertiser.campaigns.each do |campaign| %>
  <%= render "layouts/dashboard/campaign_chart", campaign: campaign %>
<% end %>
